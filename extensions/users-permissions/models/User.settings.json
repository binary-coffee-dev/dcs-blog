{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "users-permissions_user",
  "info": {
    "name": "user",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "model": "role",
      "via": "users",
      "plugin": "users-permissions",
      "configurable": false
    },
    "avatar": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "avatarUrl": {
      "type": "string",
      "maxLength": 100
    },
    "name": {
      "type": "string",
      "required": true,
      "maxLength": 30,
      "minLength": 2
    },
    "page": {
      "type": "string"
    },
    "providers": {
      "via": "user",
      "collection": "provider"
    },
    "images": {
      "via": "user",
      "collection": "image"
    },
    "comments": {
      "via": "user",
      "collection": "comment"
    },
    "opinions": {
      "via": "user",
      "collection": "opinion"
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    }
  }
}
