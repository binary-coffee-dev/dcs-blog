{
  "connection": "default",
  "collectionName": "providers",
  "info": {
    "name": "provider"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "provider": {
      "type": "string"
    },
    "scope": {
      "type": "string"
    },
    "username": {
      "type": "string"
    },
    "avatar": {
      "type": "string"
    },
    "url": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "token": {
      "private": true,
      "type": "string"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "providers"
    }
  }
}
