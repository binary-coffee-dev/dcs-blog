{
  "connection": "default",
  "collectionName": "post",
  "info": {
    "name": "post",
    "description": ""
  },
  "options": {
    "timestamps": [
      "createdAt",
      "updatedAt"
    ]
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "body": {
      "type": "richtext"
    },
    "author": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "description": {
      "type": "text",
      "maxLength": 300
    },
    "banner": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "enable": {
      "type": "boolean"
    },
    "name": {
      "unique": true,
      "type": "string"
    },
    "views": {
      "type": "biginteger",
      "default": "0"
    },
    "publishedAt": {
      "type": "datetime"
    }
  }
}
