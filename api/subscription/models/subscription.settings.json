{
  "connection": "default",
  "collectionName": "subscriptions",
  "info": {
    "name": "subscription"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "email": {
      "type": "email"
    },
    "enable": {
      "type": "boolean",
      "default": true
    },
    "verified": {
      "type": "boolean",
      "default": false
    },
    "token": {
      "type": "string"
    }
  }
}
