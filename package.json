    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "check": "npm run test && npm run lint && npm run build"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "chai-spies": "^1.0.0",
    "nyc": "^15.0.0",
    "mocha": "^10.2.0",
    "sqlite3": "^5.1.6"
  },
  "dependencies": {
    "@strapi/strapi": "4.1.2",
    "@strapi/plugin-graphql": "4.1.2",
    "@strapi/plugin-users-permissions": "4.1.2",
    "@strapi/provider-email-nodemailer": "4.1.2",
    "axios": "^0.21.4",
    "ejs": "^3.0.1",
    "eslint": "^6.8.0",
    "fast-xml-parser": "^3.16.0",
    "feed": "^4.1.0",
    "html-minifier": "^4.0.0",
    "marked": "^2.0.3",
    "moment": "^2.26.0",
    "moment-timezone": "^0.5.31",
    "mysql": "^2.18.1",
